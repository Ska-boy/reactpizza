}, [activeType, activeSize]);\r\n\r\n  const onSelectType = (index) => {\r\n    setactiveType(index);\r\n  };\r\n  const onSelectSize = (index) => {\r\n    setactiveSize(index);\r\n  };\r\n\r\n  const onClickAdd = (item) => {\r\n    props.onAddCart(item);\r\n  };\r\n\r\n  return (\r\n    <div className=\"pizza-block\">\r\n      <img className=\"pizza-block__image\" src={props.imageUrl} alt=\"Pizza\" />\r\n      <h4 className=\"pizza-block__title\">{props.name}</h4>\r\n      <div className=\"pizza-block__selector\">\r\n        <ul>\r\n          {typesNames.map((type, index) => (\r\n            <li\r\n              key={type}\r\n              onClick={() => onSelectType(index)}\r\n              className={classNames({\r\n                active: activeType == index,\r\n                disabled: !props.types.includes(index),\r\n              })}>\r\n              {type}\r\n            </li>\r\n          ))}\r\n        </ul>\r\n        <ul>\r\n          {sizes.map((size, index) => (\r\n            <li\r\n              key={size}\r\n              onClick={() => onSelectSize(size)}\r\n              className={classNames({\r\n                active: activeSize == size,\r\n                disabled: !props.sizes.includes(size),\r\n              })}>\r\n              {size} см.\r\n            </li>\r\n          ))}\r\n        </ul>\r\n      </div>\r\n      <div className=\"pizza-block__bottom\">\r\n        <div className=\"pizza-block__price\">от {props.price} ₽</div>\r\n        <div onClick={() => onClickAdd(handleAdd)} className=\"button button--outline button--add\">\r\n          <svg\r\n            width=\"12\"\r\n            height=\"12\"\r\n            viewBox=\"0 0 12 12\"\r\n            fill=\"none\"\r\n            xmlns=\"http://www.w3.org/2000/svg\">\r\n            <path\r\n              d=\"M10.8 4.8H7.2V1.2C7.2 0.5373 6.6627 0 6 0C5.3373 0 4.8 0.5373 4.8 1.2V4.8H1.2C0.5373 4.8 0 5.3373 0 6C0 6.6627 0.5373 7.2 1.2 7.2H4.8V10.8C4.8 11.4627 5.3373 12 6 12C6.6627 12 7.